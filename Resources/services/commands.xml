<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <service id="Shopware\Commands\SwagImportExport\ImportCommand">
            <argument type="service" id="Shopware\Components\SwagImportExport\Factories\ProfileFactory"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\UploadPathProvider"/>
            <argument type="service" id="models"/>
            <argument type="service" id="Shopware\CustomModels\ImportExport\ProfileRepository"/>

            <tag name="console.command" command="sw:importexport:import"/>
        </service>

        <service id="Shopware\Commands\SwagImportExport\ExportCommand">
            <argument type="service" id="Shopware\CustomModels\ImportExport\ProfileRepository"/>
            <argument type="service" id="models"/>
            <argument>%kernel.root_dir%</argument>

            <tag name="console.command" command="sw:importexport:export"/>
        </service>

        <service id="Shopware\Commands\SwagImportExport\ProfilesCommand">
            <argument type="service" id="Shopware\CustomModels\ImportExport\ProfileRepository"/>
            <argument type="service" id="models"/>

            <tag name="console.command" command="sw:importexport:profiles"/>
        </service>
    </services>
</container>
