<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
<services>
    <service id="SwagImportExport\Components\Factories\DataFactory" public="true">
        <argument type="service" id="events"/>
        <argument type="service" id="models"/>
        <argument type="tagged" tag="import_export.db_adapter"/>
        <argument type="tagged" tag="import_export.data_manager"/>
        <argument type="service" id="SwagImportExport\Components\UploadPathProvider"/>

        <tag name="import_export.proxyable"/>
    </service>

    <service id="SwagImportExport\Components\Factories\ProfileFactory" public="true">
        <argument type="service" id="models"/>
        <argument type="service" id="events"/>

        <tag name="import_export.proxyable"/>
    </service>

    <service id="SwagImportExport\Components\Factories\FileIOFactory" public="true">
        <argument type="tagged" tag="import_export.file_writer"/>
        <argument type="tagged" tag="import_export.file_reader"/>

        <tag name="import_export.proxyable"/>
    </service>

    <service id="SwagImportExport\Components\Factories\DataTransformerFactory" public="true">
        <argument type="tagged" tag="import_export.transformers"/>

        <tag name="import_export.proxyable"/>
    </service>
</services>
</container>
