<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Shopware\Components\SwagImportExport\DbAdapters\AddressDbAdapter" public="true">
            <argument type="service" id="models"/>

            <tag name="import_export.proxyable"/>
        </service>

        <service id="Shopware\Components\SwagImportExport\DbAdapters\ArticlesDbAdapter" public="true">
            <argument type="service" id="service_container"/>
            <argument type="service" id="db"/>
            <argument type="service" id="models"/>
            <argument type="service" id="shopware_media.media_service"/>
            <argument type="service" id="config"/>
            <argument type="service" id="events"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\Service\UnderscoreToCamelCaseService"/>
            <argument type="service" id="shopware_storefront.context_service"/>
            <argument type="service" id="shopware_product_stream.repository"/>
            <argument type="service" id="shopware_search.product_number_search"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\DbAdapters\Articles\ArticleWriter"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\DbAdapters\Articles\PriceWriter"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\DbAdapters\Articles\CategoryWriter"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\DbAdapters\Articles\ConfiguratorWriter"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\DbAdapters\Articles\TranslationWriter"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\DbAdapters\Articles\PropertyWriter"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\DbAdapters\Articles\RelationWriter"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\DbAdapters\Articles\ImageWriter"/>

            <tag name="import_export.proxyable"/>
        </service>

        <service id="Shopware\Components\SwagImportExport\DbAdapters\ArticlesImagesDbAdapter" public="true">
            <argument type="service" id="models"/>
            <argument type="service" id="db"/>
            <argument type="service" id="shopware_media.media_service"/>
            <argument type="service" id="events"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\DataManagers\ArticleImageDataManager"/>
            <argument type="service" id="thumbnail_manager"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\Service\UnderscoreToCamelCaseService"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\DbalHelper"/>
            <argument type="service" id="guzzle_http_client_factory"/>

            <tag name="import_export.proxyable"/>
        </service>
    </services>
</container>
