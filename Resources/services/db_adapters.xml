<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="SwagImportExport\Components\DbAdapters\AddressDbAdapter" public="true">
            <argument type="service" id="models"/>
            <argument type="service" id="config"/>

            <tag name="import_export.proxyable"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\ArticlesDbAdapter" public="true">
            <argument type="service" id="service_container"/>
            <argument type="service" id="db"/>
            <argument type="service" id="models"/>
            <argument type="service" id="shopware_media.media_service"/>
            <argument type="service" id="config"/>
            <argument type="service" id="events"/>
            <argument type="service" id="SwagImportExport\Components\Service\UnderscoreToCamelCaseService"/>
            <argument type="service" id="shopware_storefront.context_service"/>
            <argument type="service" id="shopware_product_stream.repository"/>
            <argument type="service" id="shopware_search.product_number_search"/>
            <argument type="service" id="SwagImportExport\Components\DbAdapters\Articles\ArticleWriter"/>
            <argument type="service" id="SwagImportExport\Components\DbAdapters\Articles\PriceWriter"/>
            <argument type="service" id="SwagImportExport\Components\DbAdapters\Articles\CategoryWriter"/>
            <argument type="service" id="SwagImportExport\Components\DbAdapters\Articles\ConfiguratorWriter"/>
            <argument type="service" id="SwagImportExport\Components\DbAdapters\Articles\TranslationWriter"/>
            <argument type="service" id="SwagImportExport\Components\DbAdapters\Articles\PropertyWriter"/>
            <argument type="service" id="SwagImportExport\Components\DbAdapters\Articles\RelationWriter"/>
            <argument type="service" id="SwagImportExport\Components\DbAdapters\Articles\ImageWriter"/>

            <tag name="import_export.proxyable"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\ArticlesImagesDbAdapter" public="true">
            <argument type="service" id="models"/>
            <argument type="service" id="db"/>
            <argument type="service" id="shopware_media.media_service"/>
            <argument type="service" id="events"/>
            <argument type="service" id="SwagImportExport\Components\DataManagers\ArticleImageDataManager"/>
            <argument type="service" id="thumbnail_manager"/>
            <argument type="service" id="SwagImportExport\Components\Service\UnderscoreToCamelCaseService"/>
            <argument type="service" id="SwagImportExport\Components\DbalHelper"/>
            <argument type="service" id="guzzle_http_client_factory"/>
            <argument type="service" id="config"/>
            <argument type="service" id="front"/>
            <argument>%kernel.root_dir%</argument>

            <tag name="import_export.proxyable"/>

        </service>

        <service id="SwagImportExport\Components\DbAdapters\ArticlesInStockDbAdapter" public="true">
            <argument type="service" id="SwagImportExport\Components\Utils\SnippetsHelper"/>
            <argument type="service" id="models"/>
            <argument type="service" id="events"/>
            <argument type="service" id="config"/>

            <tag name="import_export.proxyable"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\ArticlesPricesDbAdapter" public="true">
            <argument type="service" id="SwagImportExport\Components\DataManagers\ArticlePriceDataManager"/>
            <argument type="service" id="models"/>
            <argument type="service" id="shopware_storefront.context_service"/>
            <argument type="service" id="shopware_search.store_front_criteria_factory"/>
            <argument type="service" id="shopware_product_stream.repository"/>
            <argument type="service" id="shopware_search.product_number_search"/>
            <argument type="service" id="events"/>
            <argument type="service" id="config"/>

            <tag name="import_export.proxyable"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\ArticlesTranslationsDbAdapter" public="true">
            <argument type="service" id="models"/>
            <argument type="service" id="translation"/>
            <argument type="service" id="db"/>
            <argument type="service" id="events"/>
            <argument type="service" id="config"/>

            <tag name="import_export.proxyable"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\CategoriesDbAdapter" public="true">
            <argument type="service" id="models"/>
            <argument type="service" id="SwagImportExport\Components\DataManagers\CategoriesDataManager"/>
            <argument type="service" id="db"/>
            <argument type="service" id="SwagImportExport\Components\Service\UnderscoreToCamelCaseService"/>
            <argument type="service" id="events"/>
            <argument type="service" id="config"/>

            <tag name="import_export.proxyable"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\CategoryTranslationDbAdapter" public="true">
            <argument type="service" id="translation"/>
            <argument type="service" id="models"/>
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="events"/>
            <argument type="service" id="config"/>

            <tag name="import_export.proxyable"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\CustomerCompleteDbAdapter" public="true">
            <argument type="service" id="models"/>
            <argument type="service" id="db"/>
            <argument type="service" id="SwagImportExport\Components\DataManagers\CustomerDataManager"/>
            <argument type="service" id="passwordencoder"/>
            <argument type="service" id="config"/>
            <argument type="service" id="events"/>
            <argument type="service" id="SwagImportExport\Components\Service\UnderscoreToCamelCaseService"/>

            <tag name="import_export.proxyable"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\CustomerDbAdapter" public="true">
            <argument type="service" id="models"/>
            <argument type="service" id="db"/>
            <argument type="service" id="SwagImportExport\Components\DataManagers\CustomerDataManager"/>
            <argument type="service" id="passwordencoder"/>
            <argument type="service" id="config"/>
            <argument type="service" id="events"/>
            <argument type="service" id="SwagImportExport\Components\Service\UnderscoreToCamelCaseService"/>

            <tag name="import_export.proxyable"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\MainOrdersDbAdapter" public="true">
            <argument type="service" id="db"/>
            <argument type="service" id="models"/>
            <argument type="service" id="SwagImportExport\Components\Service\UnderscoreToCamelCaseService"/>
            <argument type="service" id="shopware.components.state_translator"/>
            <argument type="service" id="config"/>

            <tag name="import_export.proxyable"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\NewsletterDbAdapter" public="true">
            <argument type="service" id="models"/>
            <argument type="service" id="SwagImportExport\Components\DataManagers\NewsletterDataManager"/>
            <argument type="service" id="db"/>
            <argument type="service" id="config"/>
            <argument type="service" id="events"/>

            <tag name="import_export.proxyable"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\OrdersDbAdapter" public="true">
            <argument type="service" id="models"/>
            <argument type="service" id="SwagImportExport\Components\Service\UnderscoreToCamelCaseService"/>
            <argument type="service" id="events"/>
            <argument type="service" id="config"/>

            <tag name="import_export.proxyable"/>

        </service>

        <service id="SwagImportExport\Components\DbAdapters\TranslationsDbAdapter" public="true">
            <argument type="service" id="models"/>
            <argument type="service" id="events"/>
            <argument type="service" id="translation"/>
            <argument type="service" id="config"/>
            <argument type="service" id="db"/>

            <tag name="import_export.proxyable"/>
        </service>
    </services>
</container>
