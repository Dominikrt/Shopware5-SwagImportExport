<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="SwagImportExport\Components\DbAdapters\Articles\ArticleWriter" public="true">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="db"/>
            <argument type="service" id="SwagImportExport\Components\DbalHelper"/>
            <argument type="service"
                      id="SwagImportExport\Components\DataManagers\Articles\ArticleDataManager"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\Articles\CategoryWriter" public="true">
            <argument type="service" id="db"/>
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="events"/>
            <argument type="service" id="categorysubscriber"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\Articles\ConfiguratorWriter" public="true">
            <argument type="service" id="SwagImportExport\Components\DbalHelper"/>
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="db"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\Articles\ImageWriter" public="true">
            <argument type="service" id="db"/>
            <argument type="service" id="dbal_connection"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\Articles\PriceWriter" public="true">
            <argument type="service" id="db"/>
            <argument type="service" id="SwagImportExport\Components\DbalHelper"/>
            <argument type="service" id="SwagImportExport\Components\DataManagers\Articles\PriceDataManager"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\Articles\PropertyWriter" public="true">
            <argument type="service" id="SwagImportExport\Components\DbalHelper"/>
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="db"/>
            <argument type="service" id="SwagImportExport\Components\Utils\SnippetsHelper"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\Articles\RelationWriter" public="true">
            <argument type="service" id="db"/>
            <argument type="service" id="dbal_connection"/>
        </service>

        <service id="SwagImportExport\Components\DbAdapters\Articles\TranslationWriter" public="true">
            <argument type="service" id="models"/>
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="translation"/>
        </service>
    </services>
</container>
