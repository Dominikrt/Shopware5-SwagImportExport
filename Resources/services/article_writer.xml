<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Shopware\Components\SwagImportExport\DbAdapters\Articles\ArticleWriter" public="true">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="db"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\DbalHelper"/>
            <argument type="service"
                      id="Shopware\Components\SwagImportExport\DataManagers\Articles\ArticleDataManager"/>
        </service>

        <service id="Shopware\Components\SwagImportExport\DbAdapters\Articles\CategoryWriter" public="true">
            <argument type="service" id="db"/>
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="events"/>
        </service>

        <service id="Shopware\Components\SwagImportExport\DbAdapters\Articles\ConfiguratorWriter" public="true">
            <argument type="service" id="Shopware\Components\SwagImportExport\DbalHelper"/>
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="db"/>
        </service>

        <service id="Shopware\Components\SwagImportExport\DbAdapters\Articles\ImageWriter" public="true">
            <argument type="service" id="db"/>
            <argument type="service" id="dbal_connection"/>
        </service>

        <service id="Shopware\Components\SwagImportExport\DbAdapters\Articles\PriceWriter" public="true">
            <argument type="service" id="db"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\DbalHelper"/>
            <argument type="service" id="Shopware\Components\SwagImportExport\DataManagers\Articles\PriceDataManager"/>
        </service>
    </services>
</container>
